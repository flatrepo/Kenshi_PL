#. Id: 15514-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15514-Dialogue.mod:0
msgid "Do not fear death, frail /FLATSKIN/. Fear the dishonor of cowardice."
msgstr ""

#. Id: 15514-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (15446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15514-Dialogue.mod:1
msgid "Your small life is like a drop in the ocean, /FLATSKIN/, it is nothing to be feared. Death has an end... dishonor does not."
msgstr ""

#. Id: 15514-Dialogue.mod
#. -- Index: 6.2
#. -- Parent line: 5 (15446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15514-Dialogue.mod:2
msgid "Honor before death, /FLATSKIN/. Because death is nothing."
msgstr ""

#. Id: 15514-Dialogue.mod
#. -- Index: 6.3
#. -- Parent line: 5 (15446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15514-Dialogue.mod:3
msgid "Honor before death, /FLATSKIN/. Because death is nothing... Your honor, however, is everything. I mourn for your loss of it."
msgstr ""

#. Id: 15514-Dialogue.mod
#. -- Index: 6.4
#. -- Parent line: 5 (15446-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15514-Dialogue.mod:4
msgid "Death is nothing, /FLATSKIN/. But the dishonour of cowardice is real."
msgstr ""

#. Id: 15515-Dialogue.mod
#. -- Index: 7.0
#. -- Parent line: 6 (15514-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15515-Dialogue.mod:0
msgid "Live forever with your shame, /CRAVEN/."
msgstr ""

#. Id: 15515-Dialogue.mod
#. -- Index: 7.1
#. -- Parent line: 6 (15514-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15515-Dialogue.mod:1
msgid "Let your shame forever hang over your head, /CRAVEN/."
msgstr ""

#. Id: 15515-Dialogue.mod
#. -- Index: 7.2
#. -- Parent line: 6 (15514-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek attack
#: 15515-Dialogue.mod:2
msgid "Your disgrace will forever loom over your head, /CRAVEN/."
msgstr ""

