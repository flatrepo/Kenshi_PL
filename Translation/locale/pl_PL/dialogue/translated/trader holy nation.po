#. Id: 42994-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:0
msgid "I don't do business with thieves!"
msgstr "Nie robię interesów ze złodziejami!"

#. Id: 42994-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:1
msgid "You think I'd trade with a thief?! Leave."
msgstr "Myślisz że będę handlować ze złodziejem?! Odejdź."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:2
msgid "Take your custom elsewhere, sinner. "
msgstr "Kupuj u kogo innego, grzeszniku."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:3
msgid "I don't deal with thieves!"
msgstr "Nie handluję ze złodziejami!"

#. Id: 42994-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:4
msgid "Sticky fingered sinner! Take your business elsewhere."
msgstr "Lepkoręki grzesznik! Rób interesy w innym miejscu."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:5
msgid "Not in my shop, thief. Take your Cats elsewhere."
msgstr "Nie w moim sklepie, złodzieju. Bierz pieniądze gdzie indziej."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:6
msgid "Really, you want to trade? After you stole from me!? Get out of my shop!"
msgstr "Naprawdę, chcesz ze mną handlować? Pomimo okradzenia mnie?! Wynoś się ze sklepu!"

#. Id: 42994-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:7
msgid "You stole from me. I won't forget that. Leave."
msgstr "Okradłeś mnie, nie zapomnę ci tego. Wynoś się."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:8
msgid "Leave, thief. I got no wares for you."
msgstr "Odejdź, złodzieju. Nic dla ciebie nie mam."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:9
msgid "I've got nothing to trade with you, thief."
msgstr "Nic ci nie sprzedam, złodzieju."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:10
msgid "Looks like I'm all sold out. Leave, thief."
msgstr "Chyba się wyprzedałem. Odejdź, złodzieju."

#. Id: 42994-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 42994-Dialogue.mod:11
msgid "I won't waste my breath on a lowly thief. Leave my shop."
msgstr "Nie będę marnować oddechu na bękarciego złodzieja. Opuść mój sklep."

#. Id: 43213-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 43213-Dialogue.mod:0
msgid "Okran bless you. How may I serve you holy /BROTHER/?"
msgstr "Niech Okran cię błogosałwi, jak mogę ci pomóc,/BROTHER/?"

#. Id: 43213-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 43213-Dialogue.mod:1
msgid "How may I serve you, holy /BROTHER/?"
msgstr "Co mogę dla ciebie zrobić, /BROTHER/?"

#. Id: 43213-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#: 43213-Dialogue.mod:2
msgid "Whatever you need, holy /BROTHER/. I'm here to serve."
msgstr "Czego tylko potrzebujesz, /BROTHER/. Jestem tu by służyć."

#. Id: 43216-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: ME, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 43216-Dialogue.mod:1
msgid "... Please leave"
msgstr "... Odejdź, proszę."

#. Id: 43216-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: ME, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 43216-Dialogue.mod:2
msgid "... I will not serve a thief"
msgstr "Nie będę usługiwać złodziejowi."

#. Id: 43216-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: trader holy nation
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: ME, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: ME, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#: 43216-Dialogue.mod:3
msgid "... Please, leave me, thief"
msgstr "Odejdź, złodzieju, nalegam."

