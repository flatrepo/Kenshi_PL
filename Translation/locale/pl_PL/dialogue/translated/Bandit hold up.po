#. Id: 14012-Escapes.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:0
msgid "Hand over your /CATS/ if you wanna live!"
msgstr "Dawaj swoje /CATS/ jeśli chcesz żyć!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:1
msgid "Give up your /CATS/ if you wanna live!"
msgstr "Oddawaj swoje /CATS/ jeśli chcesz przeżyć!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:2
msgid "Give us your /CATS/ if you wanna live!"
msgstr "Chcesz żyć? Dawaj /CATS/!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:3
msgid "Hand over your /CATS/ or I'll /SLITYOURTHROAT/!"
msgstr "Dawaj /CATS/ albo poderżnę ci gardło!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:4
msgid "Give up your /CATS/ or I'll /SLITYOURTHROAT/!"
msgstr "Oddawaj /CATS/ albo cię zarżnę!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:5
msgid "Give us your /CATS/ or I'll /SLITYOURTHROAT/!"
msgstr "Daj nam swoje /CATS/ albo cię rozjebię!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:6
msgid "Money! Hand it over, /INSULT/!"
msgstr "Kasa! Dawaj ją, /INSULT/!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:7
msgid "/CATS/! Hand 'em over, /INSULT/!"
msgstr "/CATS/! Dawaj je, /INSULT/!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:8
msgid "Hand over you Cats! Don't make me have to pry 'em from your /CORPSE/, /INSULT/!"
msgstr "Dawaj swoje catuny! Nie będę ich szukać w twoim martwym ciele, /INSULT/!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:9
msgid "Hand over you Cats or /THREAT/!"
msgstr "Dawaj catuny albo cię zapierdolę!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:10
msgid "Give up your /CATS/ or /THREAT/!"
msgstr "Oddawaj catuny albo zdechniesz!"

#. Id: 14012-Escapes.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14012-Escapes.mod:11
msgid "/CATS/! Hand 'em over or /THREAT/!"
msgstr "/CATS/! Dawaj je albo zdechniesz!"

#. Id: 14013-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY>500, Tag: -1
#: 14013-Escapes.mod:0
msgid "Take it! (Give 500 Cats)"
msgstr "Bierz je! (Daj 500 c.)"

#. Id: 14013-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY>500, Tag: -1
#: 14013-Escapes.mod:1
msgid "Okay, okay! (Give 500 Cats)"
msgstr "Dobra, dobra! (Daj 500 c.)"

#. Id: 14013-Escapes.mod
#. -- Index: 1.2
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY>500, Tag: -1
#: 14013-Escapes.mod:2
msgid "/FINE/... (Give 500 Cats)"
msgstr "Ech, /FINE/... (Daj 500 c.)"

#. Id: 14018-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14013-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14018-Escapes.mod:0
msgid "Good, quick learnin', /INSULT/"
msgstr "Szybko się uczysz, /INSULT/."

#. Id: 14018-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14013-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14018-Escapes.mod:1
msgid "Smart choice, /BOY/"
msgstr "Mądry wybór, /BOY/."

#. Id: 14018-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (14013-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. Id: 16802-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Prison break bandit
#. -- Condition: In town(s) of faction(s): Święty Naród or Gildia Handlowa or Zjednoczone Miasta
#: 14018-Escapes.mod:2 16802-Dialogue.mod:0
msgid "Good /BOY/!"
msgstr "Dobrze, /BOY/!"

#. Id: 14018-Escapes.mod
#. -- Index: 2.3
#. -- Parent line: 1 (14013-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14018-Escapes.mod:3
msgid "You learn well, /BOY/"
msgstr "Dobrze się uczysz, /BOY/."

#. Id: 14017-Escapes.mod
#. -- Index: 3.0
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14017-Escapes.mod:0
msgid "Just don't try comin' after us!"
msgstr "Nie próbuj iść za nami!"

#. Id: 14017-Escapes.mod
#. -- Index: 3.1
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14017-Escapes.mod:1
msgid "Don't even think about comin' after us!"
msgstr "Nawet nie myśl o ściganiu nas!"

#. Id: 14017-Escapes.mod
#. -- Index: 3.2
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14017-Escapes.mod:2
msgid "I'll let you live another day!"
msgstr "Pozwolę ci żyć kolejny dzień!"

#. Id: 14017-Escapes.mod
#. -- Index: 3.3
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14017-Escapes.mod:3
msgid "You get to live another day!"
msgstr "Możesz teraz żyć kolejny dzień!"

#. Id: 14017-Escapes.mod
#. -- Index: 3.4
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14017-Escapes.mod:4
msgid "Now /GETLOST/!"
msgstr "A teraz spadaj!"

#. Id: 14017-Escapes.mod
#. -- Index: 3.5
#. -- Parent line: 2 (14018-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. Id: 9980-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (9979-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Territorial thug (Dancemonkey pt4)
#: 14017-Escapes.mod:5 9980-Dialogue.mod:0
msgid "Now get outta my face!"
msgstr "Wynoś się stąd!"

#. Id: 14014-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY>300, Tag: -1
#: 14014-Escapes.mod:0
msgid "Here (Give 300 Cats)"
msgstr "Masz. (Daj 300 c.)"

#. Id: 14014-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY>300, Tag: -1
#: 14014-Escapes.mod:1
msgid "/FINE/ (Give 300 Cats)"
msgstr "Ech, /FINE/. (Daj 300 c.)"

#. Id: 14019-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 14019-Escapes.mod:0
msgid "300 Cats eh? And what about them backpacks o' yours? You ain't got more than 300 Cats in all them hidey holes?! Empty 'em out! Lying /BASTARD/! "
msgstr "300 catunów, co? A co z tymi torbami? Na pewno nie masz tam więcej niż 300 catunów?! Opróżniaj je, kłamliwa szmato!"

#. Id: 14019-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS>1, Tag: -1
#: 14019-Escapes.mod:1
msgid "You sure you don't have any extra Cats hidden in them backpacks!? /FUCKING/ /MISER/... Let me see!"
msgstr "Na pewno nie masz żadnych pieniędzy w tych torbach? Cholerny przegryw... Sam sprawdzę!"

#. Id: 14578-Dialogue (4).mod
#. -- Index: 3.0
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14578-Dialogue?(4).mod:0
msgid "It's all I've got, I swear"
msgstr "Tylko tyle mam, przysięgam!"

#. Id: 14578-Dialogue (4).mod
#. -- Index: 3.1
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14578-Dialogue?(4).mod:1
msgid "It's all I have, honestly"
msgstr "Szczerze, to nie mam więcej..."

#. Id: 14578-Dialogue (4).mod
#. -- Index: 3.2
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14578-Dialogue?(4).mod:2
msgid "I swear, I don't have any more money"
msgstr "Przysięgam, że nie mam więcej kasy."

#. Id: 14580-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (14578-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14580-Dialogue?(4).mod:0
msgid "Tch, /FINE/. Good for nothing bum. I'm taking the 300 though, you're lucky I'm in a good mood!"
msgstr "Pff, /FINE/. Lepsze to niż nic, ciesz się że mam dobry humor!"

#. Id: 14581-Dialogue (4).mod
#. -- Index: 5.0
#. -- Parent line: 4 (14580-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14581-Dialogue?(4).mod:0
msgid "Now get the hell outta my face before I change my mind"
msgstr "A teraz wynoś się stąd zanim zmienię zdanie!"

#. Id: 14581-Dialogue (4).mod
#. -- Index: 5.1
#. -- Parent line: 4 (14580-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14581-Dialogue?(4).mod:1
msgid "Now /GETLOST/. Before I change my mind."
msgstr "A teraz spadaj, bo zmienię zdanie!"

#. Id: 14583-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (14578-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14583-Dialogue?(4).mod:0
msgid "Lyin' /IDIOT/!"
msgstr "/IDIOT/!"

#. Id: 14584-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (14578-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14584-Dialogue?(4).mod:0
msgid "Urgh, lying /RUNT/! Just for that, I'm takin' your /MONEY/ AND givin' you a /STABBIN/!"
msgstr "Uch, kłamliwy śmieć! Za to zabiorę ci i kasę, i nerki!"

#. Id: 15332-Dialogue.mod
#. -- Index: 5.4
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15332-Dialogue.mod:4
msgid "Get over 'ere!"
msgstr "Cho no tu!"

#. Id: 15334-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 15334-Dialogue.mod:0
msgid "Come to daddy"
msgstr "Chodź do tatusia!"

#. Id: 15334-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#: 15334-Dialogue.mod:1
msgid "Come to papa"
msgstr "Chodź do tatki!"

#. Id: 15336-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15336-Dialogue.mod:0
msgid "I'll sort 'em out!"
msgstr "Ja się tym zajmę!"

#. Id: 15336-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15336-Dialogue.mod:1
msgid "Nowhere to hide, /INSULT/!"
msgstr "Nie masz się gdzie schować, /INSULT/!"

#. Id: 15336-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15336-Dialogue.mod:2
msgid "Nowhere to run, /INSULT/!"
msgstr "Nie masz gdzie uciec, /INSULT/!"

#. Id: 15336-Dialogue.mod
#. -- Index: 5.3
#. -- Parent line: 4 (14584-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15336-Dialogue.mod:3
msgid "Chase 'em down!"
msgstr "Gonić ich!"

#. Id: 14579-Dialogue (4).mod
#. -- Index: 3.0
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14579-Dialogue?(4).mod:0
msgid "Get your /GODDAMN/ beak out of my bag!"
msgstr "Zabieraj swój cholerny dziób od moich toreb!"

#. Id: 14579-Dialogue (4).mod
#. -- Index: 3.1
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14579-Dialogue?(4).mod:1
msgid "Get your /GODDAMN/ fingers out of my bag!"
msgstr "Zabieraj łapska ode mnie!"

#. Id: 14579-Dialogue (4).mod
#. -- Index: 3.2
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14579-Dialogue?(4).mod:2
msgid "Don't touch my /GODDAMN/ bag!"
msgstr "Nie ruszaj cholernej torby!"

#. Id: 14579-Dialogue (4).mod
#. -- Index: 3.3
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14579-Dialogue?(4).mod:3
msgid "Hey back off! Don't touch my stuff!"
msgstr "Hej, cofnij się! To moje rzeczy!"

#. Id: 14591-Dialogue (4).mod
#. -- Index: 4.1
#. -- Parent line: 3 (14579-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14591-Dialogue?(4).mod:1
msgid "Stubborn /IDIOT/!"
msgstr "Uparty cwel!"

#. Id: 15317-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15317-Dialogue.mod:0
msgid "Go ahead, check my belongings"
msgstr "No dobra, sprawdź sobie."

#. Id: 15318-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15318-Dialogue.mod:0
msgid "Don't mind if I do..."
msgstr "No dobra, nie przeszkadzaj..."

#. Id: 15318-Dialogue.mod
#. -- Index: 4.1
#. -- Parent line: 3 (15317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15318-Dialogue.mod:1
msgid "Alright, I will..."
msgstr "Okej, zrobię to..."

#. Id: 15318-Dialogue.mod
#. -- Index: 4.2
#. -- Parent line: 3 (15317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. Id: 15096-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15089-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Hire me Outlaw 700 - non robot
#. Id: 5592-Escapes.mod
#. -- Index: 4.2
#. -- Parent line: 3 (5461-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Cannibal neighbour safe pt 6 talk to
#: 15318-Dialogue.mod:2 15096-Dialogue.mod:0 5592-Escapes.mod:2
msgid "Alright..."
msgstr "Okej..."

#. Id: 15318-Dialogue.mod
#. -- Index: 4.3
#. -- Parent line: 3 (15317-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15318-Dialogue.mod:3
msgid "I will!..."
msgstr "Zrobię to!..."

#. Id: 15319-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PLAYERMONEY<500, Tag: -1
#: 15319-Dialogue.mod:0
msgid "/FINE/, just gimme the 300 Cats and /GETLOST/. /FUCKING/ /SCROUNGER/..."
msgstr "Dobra, dawaj te 300 catunów i zwijaj się stąd..."

#. Id: 15322-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15322-Dialogue.mod:0
msgid "/FUCKING/ liar! You lied to me! You got loads of /STUFF/ on ya!"
msgstr "Jebany kłamca! Masz dużo rzeczy ze sobą!"

#. Id: 15322-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15322-Dialogue.mod:1
msgid "/FUCKING/ liar! You lied to me! Lookit all these Cats!"
msgstr "Cholerny kłamca! Okłamałeś mnie! Spójrzcie na te catuny!"

#. Id: 15322-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15322-Dialogue.mod:2
msgid "Lying /BASTARD/! You lied to me! Lookit all these monies!"
msgstr "Kłamca! Spójrzcie na tą kasę!"

#. Id: 15325-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15325-Dialogue.mod:0
msgid "What? Liar! You got loads of sexy /STUFF/ on ya!"
msgstr "Co? Kłamca! Masz sporo fajnych rzeczy ze sobą!"

#. Id: 15325-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15325-Dialogue.mod:1
msgid "Eh? /GODDAMN/ liar! You got loads of fancy /STUFF/ in here!"
msgstr "Hę? Colerny kłamca! Masz ze sobą sporo fajnego sprzętu!"

#. Id: 15325-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (15318-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>500, Tag: -1
#: 15325-Dialogue.mod:2
msgid "Mah? Lying /INSULT/... You got loads of swanky /STUFF/ in here!"
msgstr "Co do... Kłamliwy śmieć! Masz sporo towaru ze sobą!"

#. Id: 15329-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (14019-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: NUM_BACKPACKS=0, Tag: -1
#: 15329-Dialogue.mod:0
msgid "Go ahead, check my stuff"
msgstr "No dawaj, sprawdź mój sprzęt."

#. Id: 14020-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>6, Tag: -1
#: 14020-Escapes.mod:0
msgid "So hold the /FUCKNOUN/ up. You're tellin' me, that between all of you /INSULT/s, you can't scrape together more than 300 /CATS/ between ya?"
msgstr "Czekaj, kurwa. Chcesz mi powiedzieć że wy, smiecie, wspólnie nie macie więcej niż 300 catunów?"

#. Id: 14020-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>6, Tag: -1
#: 14020-Escapes.mod:1
msgid "300 Cats?! So a pack 'o /IDIOT/s like you lot can't afford more than 300 Cats between you?! Do I really gotta start slittin' throats already?"
msgstr "300 catunów?! Banda idiotów jak wy nie potrafi zebrać więcej niż 300 catunów RAZEM!? Naprawdę mam zacząć podcinać gardła?"

#. Id: 14592-Dialogue (4).mod
#. -- Index: 3.0
#. -- Parent line: 2 (14020-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>2, Tag: -1
#: 14592-Dialogue?(4).mod:0
msgid "You'd rather fight us all, would you?"
msgstr "Wolisz walczyć z nami wszystkimi, nie?"

#. Id: 14592-Dialogue (4).mod
#. -- Index: 3.1
#. -- Parent line: 2 (14020-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>2, Tag: -1
#: 14592-Dialogue?(4).mod:1
msgid "So you'd rather fight ALL of us?"
msgstr "Więc wolisz walczyć z NAMI?"

#. Id: 14593-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (14592-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>6, Tag: -1
#. -- Condition: Who: ME, Condition: SQUAD_SIZE<9, Tag: -1
#: 14593-Dialogue?(4).mod:0
msgid "... /FINE/... Just give us the 300 Cats and /FUCKOFF/"
msgstr ".../FINE/... Daj 300 catunów i wykurwiaj."

#. Id: 14593-Dialogue (4).mod
#. -- Index: 4.1
#. -- Parent line: 3 (14592-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>6, Tag: -1
#. -- Condition: Who: ME, Condition: SQUAD_SIZE<9, Tag: -1
#: 14593-Dialogue?(4).mod:1
msgid "... /FINE/... Just hand over the 300 Cats and /FUCKOFF/"
msgstr "Ech, /FINE/, dawaj te 300 catunów i spadaj."

#. Id: 14595-Dialogue (4).mod
#. -- Index: 4.0
#. -- Parent line: 3 (14592-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14595-Dialogue?(4).mod:0
msgid "Just keep pushin' me, /IDIOT/, you /RUNT/s don't scare me. So, am I gonna have to loot the /CATS/ from your /CRAPPY/ /CORPSE/, or are ya gonna behave...?"
msgstr "Próbujcie dalej, zjeby, nie boję się. Mam wyciągać kasę z waszych durnych trupów, czy zaczniecie się zachowywać?"

#. Id: 14595-Dialogue (4).mod
#. -- Index: 4.1
#. -- Parent line: 3 (14592-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14595-Dialogue?(4).mod:1
msgid "Just give me a reason to /THREAT_ACTION/ your /BODYPART/, /IDIOT/, I'm always up for makin' /RUNT/s bleed! So are ya gonna comply, or no...?"
msgstr "Daj mi powód dla którego miałbym ci nie podżynać gardła. Zawsze mam ochotę na takie rzeczy! Więc zaczniesz wspólpracować, czy nie?"

#. Id: 14595-Dialogue (4).mod
#. -- Index: 4.2
#. -- Parent line: 3 (14592-Dialogue (4).mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14595-Dialogue?(4).mod:2
msgid "/LAUGH/, I don't give a shit how many of you there are! I'm always up for a fight! So... one last chance, are ya gonna obey?"
msgstr "/LAUGH/, gówno mnie obchodzi ile was tam jest! Zawsze mogę walczyć! Więc... Ostatnia szansa, zaczniecie się słuchać?"

#. Id: 14597-Dialogue (4).mod
#. -- Index: 5.0
#. -- Parent line: 4 (14595-Dialogue (4).mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. Id: 8691-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Bo attacked
#: 14597-Dialogue?(4).mod:0 8691-Dialogue.mod:0
msgid "Bring it on then!"
msgstr "No to stawaj!"

#. Id: 14597-Dialogue (4).mod
#. -- Index: 5.1
#. -- Parent line: 4 (14595-Dialogue (4).mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14597-Dialogue?(4).mod:1
msgid "Come on then, hit me!"
msgstr "No dawaj, uderz mnie!"

#. Id: 14597-Dialogue (4).mod
#. -- Index: 5.3
#. -- Parent line: 4 (14595-Dialogue (4).mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14597-Dialogue?(4).mod:3
msgid "Show me what you got then!"
msgstr "Pokaż więc co potrafisz!"

#. Id: 15344-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (14020-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15344-Dialogue.mod:0
msgid "Just take the 300, bandit. Don't push me."
msgstr "Bierz te 300, bandyto. Nie denerwuj mnie."

#. Id: 15344-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (14020-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15344-Dialogue.mod:1
msgid "Don't push it bandit, just take the 300"
msgstr "Nie przeginaj, gnojku, bierz 300."

#. Id: 15344-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (14020-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15344-Dialogue.mod:2
msgid "You don't want to push me, bandit..."
msgstr "Nie prowokuj mnie, bandyto..."

#. Id: 15345-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15344-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15345-Dialogue.mod:0
msgid "Looks like we got us a cocky /BASTARD/! Alright boys, it's /STABBIN/ time!"
msgstr "Wygląda na to że mamy chojraka! Dobra, koledzy, czas na dźganko!"

#. Id: 15347-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (15344-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15347-Dialogue.mod:0
msgid "/QUICKLAUGH/! And why's that?"
msgstr "/QUICKLAUGH/! Bo co?"

#. Id: 15348-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15348-Dialogue.mod:0
msgid "I use bandits like you for target practise"
msgstr "Tacy ludzie jak ty służą za tarcze dla moich strzelców."

#. Id: 15348-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15348-Dialogue.mod:1
msgid "I've killed hundreds of bandits like you"
msgstr "Setki bandytów jak ty padło pod moimi butami."

#. Id: 15348-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15348-Dialogue.mod:2
msgid "I eat bandits like you for breakfast"
msgstr "Takich jak ty zjadam na śniadanie!"

#. Id: 15353-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15353-Dialogue.mod:0
msgid "Oh yeah? We'll see how well you do against ME!"
msgstr "Doprawdy! Zatem zobaczymy jak sobie poradzisz ZE MNĄ!"

#. Id: 15353-Dialogue.mod
#. -- Index: 6.1
#. -- Parent line: 5 (15348-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15353-Dialogue.mod:1
msgid "Is that right? Then let's see how you do against ME!"
msgstr "Skoro tak, to spróbuj się ze mną!"

#. Id: 15349-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15349-Dialogue.mod:0
msgid "Because I don't like getting my hands dirty"
msgstr "Bo nie lubię brudzić sobie rąk."

#. Id: 15349-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15349-Dialogue.mod:1
msgid "Because you won't like me when I'm angry"
msgstr "Bo nie polubisz mnie, gdy się wkurwię."

#. Id: 15349-Dialogue.mod
#. -- Index: 5.2
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15349-Dialogue.mod:2
msgid "Because I'm a lover, not a fighter"
msgstr "Bo wolę kochać, niż walczyć."

#. Id: 15352-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15349-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15352-Dialogue.mod:0
msgid "/LAUGH/! Stubborn /IDIOT/!"
msgstr "/LAUGH/! Uparty /IDIOT/!"

#. Id: 15354-Dialogue.mod
#. -- Index: 6.0
#. -- Parent line: 5 (15350-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 15354-Dialogue.mod:0
msgid "Bah! You /RUNT/s don't scare me!"
msgstr "Bach, nie boję się was!"

#. Id: 15351-Dialogue.mod
#. -- Index: 5.0
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15351-Dialogue.mod:0
msgid "Keep pushing me and you'll find out soon"
msgstr "Denerwuj mnie dalej to się dowiesz..."

#. Id: 15351-Dialogue.mod
#. -- Index: 5.1
#. -- Parent line: 4 (15347-Dialogue.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 15351-Dialogue.mod:1
msgid "Keep pushing, maybe you'll find out"
msgstr "Prowokuj mnie dalej, może się dowiesz."

#. Id: 14023-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>8, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 14023-Escapes.mod:0
msgid "MORE!"
msgstr "Więcej!"

#. Id: 14023-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. -- Condition: Who: ME, Condition: SQUAD_SIZE>8, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_OUTNUMBERED=1, Tag: -1
#: 14023-Escapes.mod:1
msgid ".... Higher."
msgstr "Daj... więcej."

#. Id: 14642-Dialogue (4).mod
#. -- Index: 3.0
#. -- Parent line: 2 (14023-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#. Id: 65930-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (65917-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Crab town recruit
#: 14642-Dialogue?(4).mod:0 65930-Dialogue.mod:0
msgid "... No!"
msgstr "Ee... Nie!"

#. Id: 14026-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14026-Escapes.mod:0
msgid "Alright, hand it over then"
msgstr "Dobra, dawaj je."

#. Id: 14026-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14026-Escapes.mod:1
msgid "/FINE/, give it here then"
msgstr "No dobra, dawaj je zatem."

#. Id: 14026-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (14014-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14026-Escapes.mod:2
msgid "/FINE/, chuck it here then"
msgstr "Dobra, podawaj je."

#. Id: 14015-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14015-Escapes.mod:0
msgid "Outta my way, /GODDAMN/ /DIRTY/ bandits!"
msgstr "Zejdźcie mi z drogi, cholerni bandyci!"

#. Id: 14015-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14015-Escapes.mod:1
msgid "Hit the road, /CRAPPY/ bandits!"
msgstr "Żryjcie piach, gnojki!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14030-Escapes.mod:0
msgid "Just have to take it from your /CORPSE/!"
msgstr "Wyrwę kasę z twojego ciała!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. Id: 16136-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Swamper stealing imprisoned
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: MANIACAL
#. Id: 18303-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Town thug mugging harassment
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: TRAITOROUS
#: 14030-Escapes.mod:2 16136-Dialogue.mod:2 18303-Dialogue.mod:5
msgid "I'll chop you up!"
msgstr "Potnę cię!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.3
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14030-Escapes.mod:3
msgid "I'll /SNAP/ your head off!"
msgstr "Ukręcę ci łeb!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.4
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14030-Escapes.mod:4
msgid "I'll /THREAT_ACTION/ your face!"
msgstr "Rozjebię ci twarz!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.5
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#. Id: 12837-Dialogue (11) (2).mod
#. -- Index: 1.3
#. -- Parent line: 0 (12836-Dialogue (11) (2).mod)
#. -- Speaker: Me (0)
#. -- Group title: Merc betrayed
#. Id: 17600-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (17598-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Stranger interrupts Shek
#. -- Condition: Who: TARGET, Condition: SQUAD_ONLY_ANIMALS=0, Tag: -1
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: FEARFUL
#: 14030-Escapes.mod:5 12837-Dialogue?(11)?(2).mod:3 17600-Dialogue.mod:0
msgid "You'll die!"
msgstr "Zdechniesz!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.6
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14030-Escapes.mod:6
msgid "I'll cut you open!"
msgstr "Potnę cię na kawałki!"

#. Id: 14030-Escapes.mod
#. -- Index: 2.7
#. -- Parent line: 1 (14015-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14030-Escapes.mod:7
msgid "I'll rip your face off!"
msgstr "Urwę ci ryj!"

#. Id: 14016-Escapes.mod
#. -- Index: 1.0
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14016-Escapes.mod:0
msgid "You want Cats? Take them from me, I dare you"
msgstr "Chcesz kasy? Zatem weź je sobie, dawaj!"

#. Id: 14016-Escapes.mod
#. -- Index: 1.1
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14016-Escapes.mod:1
msgid "You want my money? Just try taking it from me!"
msgstr "Chcesz pieniędzy? Weź je sobie!"

#. Id: 14016-Escapes.mod
#. -- Index: 1.2
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14016-Escapes.mod:2
msgid "Bring it on, bandit dogs! I've been itching for a fight!"
msgstr "Dawajcie, psy! Czekam na dobrą walkę!"

#. Id: 14016-Escapes.mod
#. -- Index: 1.3
#. -- Parent line: 0 (14012-Escapes.mod)
#. -- Speaker: Target (1)
#. -- Group title: Bandit hold up
#: 14016-Escapes.mod:3
msgid "No! Come on, come at me, /DIRTY/ bandits! "
msgstr "Nie! Spróbuj się z moją bronią!"

#. Id: 14031-Escapes.mod
#. -- Index: 2.0
#. -- Parent line: 1 (14016-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14031-Escapes.mod:0
msgid "Smarmy /BASTARD/!"
msgstr "Cholerny gnój!"

#. Id: 14031-Escapes.mod
#. -- Index: 2.1
#. -- Parent line: 1 (14016-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14031-Escapes.mod:1
msgid "Cocky /BASTARD/!"
msgstr "Jebany chojrak!"

#. Id: 14031-Escapes.mod
#. -- Index: 2.2
#. -- Parent line: 1 (14016-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14031-Escapes.mod:2
msgid "I'll cut that smug look off your face, /BASTARD/!"
msgstr "Wytnę ci uśmieszek z tej mordy!"

#. Id: 14031-Escapes.mod
#. -- Index: 2.4
#. -- Parent line: 1 (14016-Escapes.mod)
#. -- Speaker: Me (0)
#. -- Group title: Bandit hold up
#: 14031-Escapes.mod:4
msgid "I'll punch that smirk off your face, /BASTARD/!"
msgstr "Wybiję ci uśmiech z gęby!"

