#. Id: 55637-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:0
msgid "My feet! I need shoes!"
msgstr "Moje stopy! Potrzebuję butów!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:1
msgid "My feet are burning!"
msgstr "Moje stopy płoną!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:2
msgid "/AAH/! My feet!"
msgstr "/AAH/! Moje stopy!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:3
msgid "My feet! The pain!"
msgstr "Moje stopy! Ten ból!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:4
msgid "My feet! The pain is killing me!"
msgstr "Moje stopy! Zdechnę z bólu!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:5
msgid "Mercy me! My feet are burning up!"
msgstr "Ratujcie, stopy mi płoną!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:6
msgid "Damn! My feet, they're burning!"
msgstr "Cholera, moje stopy płoną!"

#. Id: 55637-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Borealczyk or Shek or Meridiańczyk
#: 55637-Dialogue.mod:7
msgid "/ARG/, my feet!"
msgstr "/ARG/, moje stopy!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:0
msgid "Feels like they're about to melt right off!"
msgstr "Czuję jakby miały się zaraz roztopić!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:1
msgid "Feels like the ground is on fire!"
msgstr "Ta ziemia płonie!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:2
msgid "Keep walking barefoot like this and I won't have any feet left to walk on!"
msgstr "Jeszcze więcej chodzenia boso i nie będę mieć na czym się poruszać!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:3
msgid "I can't take this much longer... The ground is eating away my feet!"
msgstr "Nie dam rady tak dłużej... Ziemia pożera moje stopy!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:4
msgid "I can't go on like this, my feet are melting!"
msgstr "Nie dam rady, moje stopy się topią!"

#. Id: 55638-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55638-Dialogue.mod:5
msgid "I need to get off this ground! It's melting my skin off!"
msgstr "Muszę stąd uciec! Moja skóra się topi!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:0
msgid "I CAN HEAR MY SKIN FIZZING! SKIN ISN'T SUPPOSED TO FIZZ!"
msgstr "SŁYSZĘ JAK MOJA SKÓRA SKWIERCZY! SKÓRA NIE POWINNA SKWIERCZEĆ!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:1
msgid "I CAN HEAR MY FEET SIZZLING! PEOPLE AREN'T SUPPOSED TO SIZZLE!"
msgstr "MOJE STOPY SKWIERCZĄ! LUDZIE NIE MOGĄ SKWIERCZEĆ!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:2
msgid "I need my skin! I like my skin! Get me off'a this acid hell-soil!"
msgstr "Potrzebuję mojej skóry! Lubię moją skórę! Wynośmy się z tej piekielnej ziemi!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:3
msgid "The ground's made of /FUCKING/ acid!"
msgstr "Ta ziemia to pierdolony kwas!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:4
msgid "/FUCKING/ acid soil! At this rate I'll have no feet left!"
msgstr "Jebana kwasowa ziemia! Zaraz nie będę mieć stóp!"

#. Id: 55641-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55641-Dialogue.mod:5
msgid "Abort! I need shoes!"
msgstr "Zawracajmy! Potrzebuję butów!"

#. Id: 55642-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:0
msgid "This isn't good. I won't make it far on this ground..."
msgstr "Niedobrze. Nie dam rady dłużej maszerować po tej ziemi..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:1
msgid "This acid... I don't know how much longer I can last like this..."
msgstr "Ten kwas... Nie wiem jak długo jeszcze wytrzymam w tym stanie..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:2
msgid "Keep walking on this ground and I'll have no skin left..."
msgstr "Więcej chodzenia i nie będę mieć skóry..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:3
msgid "This ground ain't made for bare feet... I need shoes..."
msgstr "Ta ziemia nie służy bosym stopom... Potrzebuję butów..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:4
msgid "I can't keep walking on this... I need my skin on my feet..."
msgstr "Nie mogę tak chodzić... Potrzebuję skóry na moich stopach..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:5
msgid "This acid is eating through my feet like it's a banquet..."
msgstr "Kwas wżera się w moje stopy jak na festynie..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:6
msgid "Walking barefoot on acid... Great idea..."
msgstr "Boso po kwasie... Świetny pomysł..."

#. Id: 55642-Dialogue.mod
#. -- Index: 1.7
#. -- Parent line: 0 (55637-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: TRAITOROUS
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: DUMB
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: FEARFUL
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: MANIACAL
#: 55642-Dialogue.mod:7
msgid "Who's wise idea was it to walk across acid soil barefooted...?"
msgstr "Kto wpadł na taki światły pomysł by przechodzić przez kwasową ziemię boso...?"

#. Id: 55640-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:0
msgid "The ground burns the skin right off my soles!"
msgstr "Ziemia pali moje podeszwy!"

#. Id: 55640-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:1
msgid "Pain! My feet are being burned!"
msgstr "Ból! Moje stopy się palą!"

#. Id: 55640-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:2
msgid "Burning! /IWE/ need boots!"
msgstr "Parzy! Potrzebujemy butów!"

#. Id: 55640-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:3
msgid "This ground... it is cooking my feet as we speak..."
msgstr "Ta ziemia... Ona smaży moje stopy..."

#. Id: 55640-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:4
msgid "Rrgh!... Bare feet aren't meant for these soils..."
msgstr "Rrgh...! Nagie stopy nie są dostosowane do tych ziem..."

#. Id: 55640-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: My Race(s): Shek
#: 55640-Dialogue.mod:5
msgid "Kch!... My feet... the pain..."
msgstr "Kch...! Moje stopy... Ten ból..."

#. Id: 55648-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55640-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55648-Dialogue.mod:0
msgid "/IWE/ need to move off this ground, quickly."
msgstr "Trzeba zejść z tej ziemi, i to szybko."

#. Id: 55648-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55640-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55648-Dialogue.mod:1
msgid "/IWE/ need to find new ground, fast."
msgstr "Trzeba znaleźć inną drogę, szybko."

#. Id: 55648-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55640-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55648-Dialogue.mod:2
msgid "/IWE/ should go no further, /MYOUR/ skin won't last the journey."
msgstr "Nie można iść dalej, skóra nie wytrzyma reszty podróży."

#. Id: 55649-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:0
msgid "/OHSHIT/, my feet! They're on fire!"
msgstr "/OHSHIT/, moje stopy! One się palą!"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:1
msgid "/WHATTHEFUCK/ is up with this place!? The ground's on fire!"
msgstr "Co jest nie tak z tym pierdolonym miejscem!? Ta ziemia płonie!"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:2
msgid "/AAH/, that stings like bitch!... Are you /FUCKING/ kiddin' me, nature? Acid soil!?"
msgstr "/AAH/, kurewsko boli...! Czy ty ze mnie kpisz, jebana naturo? Kwasowa ziemia!?"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:3
msgid "/OHSHIT/, my feet! They're burning!"
msgstr "/OHSHIT/, moje stopy! One płoną!"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:4
msgid "/OHSHIT/, my feet! They're melting!"
msgstr "/OHSHIT/, moje stopy! One się topią!"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:5
msgid "/AAH/! My feet! The ground's made of /FUCKING/ acid!"
msgstr "/AAH/! Moje stopy! Ta ziemia to jebany kwas!"

#. Id: 55649-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: HONORABLE
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: SMART
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=0, Tag: BRAVE
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55649-Dialogue.mod:6
msgid "*Heave* *Heave*... My feet! My feet! It burns!"
msgstr "Moje stopy, moje stopy, moje stopy! PŁONĄ!"

#. Id: 55654-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#. -- Condition: Who: ME, Condition: PERSONALITY_TAG=1, Tag: DUMB
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55654-Dialogue.mod:0
msgid "*Sniff* *Sniff*... /OI/, what's cookin'?"
msgstr "*Wącha*... /OI/, coś się smaży?"

#. Id: 55655-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55654-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid feet tough
#: 55655-Dialogue.mod:0
msgid "M... ME! My feet are being cooked! I need shoes! Get me some /FUCKING/ shoes!"
msgstr "J... JA! Moje stopy się smażą! Potrzebuję butów! Dajcie mi jebane buty!"

