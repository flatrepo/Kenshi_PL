#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:1
msgid "/HOLYGREET/. How can I help you?"
msgstr "/HOLYGREET/. Mogę ci pomóc?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:2
msgid "/HOLYGREET/. Need supplies?"
msgstr "/HOLYGREET/. Potrzebujesz zapasów?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:3
msgid "/HOLYGREET/. Looking to trade?"
msgstr "/HOLYGREET/. Chcesz coś kupić?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:4
msgid "/HOLYGREET/. Here for business?"
msgstr "/HOLYGREET/. Chcesz handlować?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:6
msgid "/HOLYGREET/. Can I help you?"
msgstr "/HOLYGREET/. Mogę ci w czymś pomóc?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:7
msgid "Welcome. /HOLYGREET/."
msgstr "Witaj. /HOLYGREET/."

#. Id: 42970-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:8
msgid "/HOLYGREET/. Take a look around."
msgstr "/HOLYGREET/. Rozejrzyj się po sklepie."

#. Id: 42970-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:9
msgid "/HOLYGREET/. See anything you like?"
msgstr "/HOLYGREET/. Widzisz coś dla siebie?"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:10
msgid "Ah, a new buyer. /HOLYGREET/."
msgstr "Ach, nowy kupiec. /HOLYGREET/."

#. Id: 42970-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:11
msgid "/HOLYGREET/, have a browse."
msgstr "/HOLYGREET/, rozejrzyj się po przybytku!"

#. Id: 42970-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42970-Dialogue.mod:12
msgid "A customer! /HOLYGREET/!"
msgstr "Klient! /HOLYGREET/!"

#. Id: 42974-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:0
msgid "Ah, back again? /HOLYGREET/."
msgstr "Ach, wracamy? /HOLYGREET/."

#. Id: 42974-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:1
msgid "Good to see you again, /BROTHER/."
msgstr "Dobrze cię znów widzieć, /BROTHER/."

#. Id: 42974-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:2
msgid "Welcome back, /BROTHER/, take a look around."
msgstr "Witaj z powrotem, /BROTHER/, rozejrzyj się."

#. Id: 42974-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:3
msgid "Welcome back, customer. Come on in."
msgstr "Witaj znów, kliencie. Wejdź."

#. Id: 42974-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:4
msgid "Welcome back, /BROTHER/! Looking for anything in particular?"
msgstr "Witaj znów, /BROTHER/! Szukasz czegoś konkretnego?"

#. Id: 42974-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:5
msgid "Come on in, /BROTHER/. Have a browse."
msgstr "Wejdź, /BROTHER/, zobacz mój asortyment."

#. Id: 42974-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: TARGET_LAST_SEEN_X_HOURS_AGO<30, Tag: -1
#: 42974-Dialogue.mod:6
msgid "Ah, /HOLYGREET/. Back for more goods?"
msgstr "Ach, /HOLYGREET/. Wracasz po więcej dóbr!"

#. Id: 42975-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42975-Dialogue.mod:4
msgid "Come to trade?"
msgstr "Chcesz handlować?"

#. Id: 42975-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_FEMALE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: HAS_TAG=0, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_PLAYER=1, Tag: -1
#: 42975-Dialogue.mod:5
msgid "Yes, need something?"
msgstr "Potrzebujesz czegoś?"

#. Id: 42978-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:0
msgid "... Your kind aren't welcome in my shop."
msgstr "... Twoja rasa nie jest mile widziana w moim sklepie."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:1
msgid "... I don't serve your kind."
msgstr "...Nie służę twojej rasie."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#. Id: 42978-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:2 42978-Dialogue.mod:3
msgid "... Pure bloods only."
msgstr "...Tylko dla czystokrwistych."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:4
msgid "I don't sell to your kind."
msgstr "Nic nie sprzedam twojej rasie."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:5
msgid "Your money isn't welcome here, beast."
msgstr "Nie chcę twoich pieniędzy, bestio."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:6
msgid "Unshackled beasts not allowed."
msgstr "Wolne bestie nie mają tu miejsca."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:7
msgid "I only serve pure bloods."
msgstr "Służę tylko czystej krwi."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:8
msgid "You, you're no Okranite. I only serve children of the light."
msgstr "Nie jesteś Okranitą. Służę tylko dzieciom światła."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:9
msgid "Dark ones aren't welcome in my shop."
msgstr "Mroczni nie są mile widziani w moim sklepie."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:10
msgid "Take your money elsewhere, dark one. I don't want your custom."
msgstr "Zabierz swoje pieniądze, mroczny. Nie chcę cię tu."

#. Id: 42978-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=0, Tag: -1
#. -- Condition: Target Race(s): Książę Roju or Shek
#: 42978-Dialogue.mod:11
msgid "Walk on, dark one. Your Cats aren't welcome here."
msgstr "Idź dalej, mroczny. Twoje catuny nie są tu mile widziane."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:1
msgid "... Corrupting our city with your /GREASY/ ways... Take your business elsewhere. "
msgstr "...Zatruwacie nasze miasto swoimi bestialskimi mocami... Szukaj gdzie indziej."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:2
msgid "Take your business elsewhere, /GREASY/ one."
msgstr "Ubijaj interesy w innym miejscu..."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:3
msgid "Leave. "
msgstr "Odejdź."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:4
msgid "... Beasts like you should be shackled."
msgstr "Bestie twojego pokroju powinny siedzieć w kajdanach."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:5
msgid "Don't need your dark energy in here. I got enough to deal with already, thanks."
msgstr "Nie potrzebuję tu twojej mrocznej energii. Mam wystarczająco dużo problemów na głowie."

#. Id: 42979-Dialogue.mod
#. -- Index: 1.6
#. -- Parent line: 0 (42978-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#: 42979-Dialogue.mod:6
msgid "Spreading all your dark juju around my shop. No thanks."
msgstr "Roznosisz mroczną aurę w moim sklepie. Chyba podziękuję."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:0
msgid "/GREASY/ thief. You're not welcome in my shop anymore."
msgstr "Cholerny złodziej. Nie chcę cię w moim sklepie."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:1
msgid "/GREASY/ criminal. That's right, I remember your face. Leave."
msgstr "Cholerny złodziej. O tak, zapamiętam twoją twarz. Wyjdź."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:2
msgid "You again! You're not welcome here, thief."
msgstr "Znów ty! Nie chcę cię tu, złodzieju."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:3
msgid "You again... /GREASY/ thief, get out of my shop."
msgstr "Znowu ty... Cholerny złodzieju, wyjdź z mojego sklepu."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:4
msgid "You. You lost your trading rights when you stole from me!"
msgstr "Ty. W momencie okradzenia mnie odebrano ci prawa handlowe!"

#. Id: 42985-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:5
msgid "Get out, /GREASY/ thief."
msgstr "Wynoś się, cholerny złodzieju."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:6
msgid "Not you again? Leave, thief."
msgstr "A niech to, znowu ty? Wynocha, złodzieju."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:7
msgid "/GREASY/ thief, I remember you. I'm watching you."
msgstr "Złodzieju... Zapamiętam cię, wszyscy cię obserwują."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:8
msgid "I've got my eye on you, thief."
msgstr "Mam na ciebie oko, złodzieju."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:9
msgid "Touch anything and I'm calling the /GUARD/s."
msgstr "Dotknij czegokolwiek a zawołam straże."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.10
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:10
msgid "Keep your /GREASY/ fingers off my stock."
msgstr "Trzymaj swoje brudne łapska z dala od mojego towaru."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.11
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:11
msgid "Don't touch anything, thief. I've got my eye on you."
msgstr "Niczego nie dotykaj, złodzieju. Mam na ciebie oko."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.12
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:12
msgid "Don't try anything stupid, thief."
msgstr "Nie próbuj głupot, złodzieju."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.13
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:13
msgid "Okran protect me from /GREASY/ thieves"
msgstr "Okranie, chroń mnie przed złodziejami..."

#. Id: 42985-Dialogue.mod
#. -- Index: 0.14
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: shopkeep greet holy
#. -- Condition: Who: TARGET, Condition: HAS_TAG=1, Tag: STOLE_FROM_ME
#. -- Condition: Who: ME, Condition: IS_FEMALE=0, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#: 42985-Dialogue.mod:14
msgid "Okran protect my shop from sinners"
msgstr "Okran chroni mnie przed grzesznikami."

