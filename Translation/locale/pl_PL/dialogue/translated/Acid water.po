#. Id: 55772-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:0
msgid "HOT HOT HOT!"
msgstr "GORĄCE GORĄCE GORĄCE!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:1
msgid "I'm boiling alive!"
msgstr "Gotuję się żywcem!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:2
msgid "/ARG/! That's HOT!"
msgstr "/ARG/! To piecze!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:4
msgid "/ARG/, it burns!"
msgstr "/ARG/, piecze!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:5
msgid "I'm /FUCKING/ melting!"
msgstr "Roztapiam się!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:6
msgid "/AAH/, I'm melting alive!"
msgstr "/AAH/, rozpuszczam się żywcem!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.7
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:7
msgid "/AAH/! Feels like I'm on fire!"
msgstr "/AAH/! Pali niczym płomień!"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.8
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:8
msgid "Feels like I'm swimming in a /FUCKING/ fire!"
msgstr "Ja pływam w jebanym ogniu czy jak!?"

#. Id: 55772-Dialogue.mod
#. -- Index: 0.9
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55772-Dialogue.mod:9
msgid "/DAMN/, that burns!"
msgstr "Cholera, to piecze!"

#. Id: 55775-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55772-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55775-Dialogue.mod:0
msgid "Stay the hell out of the water!"
msgstr "Trzymać się z dala od wody!"

#. Id: 55775-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55772-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55775-Dialogue.mod:2
msgid "This stuff burns like a bitch!"
msgstr "To kurewsko piecze!"

#. Id: 55775-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (55772-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55775-Dialogue.mod:3
msgid "This shit burns like a bastard!"
msgstr "To gówno cholernie pali!"

#. Id: 55775-Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (55772-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55775-Dialogue.mod:4
msgid "If I don't get out fast, I'll be soup!"
msgstr "Jeśli stąd nie wyjdę zostanę zupą!"

#. Id: 55773-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55773-Dialogue.mod:1
msgid "Raargh!"
msgstr ""

#. Id: 55773-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55773-Dialogue.mod:2
msgid "Burning!"
msgstr "Piecze!"

#. Id: 55773-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55773-Dialogue.mod:3
msgid "Hot!"
msgstr "Gorące!"

#. Id: 55774-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55773-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55774-Dialogue.mod:0
msgid "It's like swimming in fire! Stay out of the water!"
msgstr "Niczym pływanie w ogniu! Nie wchodzić do wody!"

#. Id: 55774-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55773-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55774-Dialogue.mod:1
msgid "This is no water, this is acid!"
msgstr "To nie jest woda tylko kwas!"

#. Id: 55774-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (55773-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55774-Dialogue.mod:2
msgid "Liquid fire! Stay out of the water!"
msgstr "Płynny ogień! Z dala od wody!"

#. Id: 55774-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (55773-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55774-Dialogue.mod:3
msgid "The water burns! Stay out!"
msgstr "Ta woda pali! Odsunąć się!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:0
msgid "My skin! This water's eating right through me!"
msgstr "Moja skóra! Ta woda mnie pożera!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:1
msgid "/ARG/! It's like swimming in fire!"
msgstr "/ARG/! Pływam w ogniu!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:2
msgid "My skin! It burns all over!"
msgstr "Moja skóra! Wszystko piecze!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:3
msgid "/ARG/! This isn't just water, it's acid water!"
msgstr "/ARG/! To nie jest zwykła woda, tylko KWAS!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:4
msgid "/ARG/! Acid!... Feels like I'm on fire!"
msgstr "/ARG/! Kwas...! Czuję jak płonę!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:5
msgid "/ARG/, acid water!"
msgstr "/ARG/, kwaśna woda!"

#. Id: 55776-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#. -- Condition: My Race(s): Borealczyk or Meridiańczyk or Shek
#: 55776-Dialogue.mod:6
msgid "This isn't water... it's acid!... The burning!"
msgstr "To nie jest woda... To kwas! BOLI!"

#. Id: 55777-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (55776-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55777-Dialogue.mod:0
msgid "Stay in here much longer and I'm soup!"
msgstr "Jeszcze trochę i będę zupą!"

#. Id: 55777-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (55776-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Acid water
#: 55777-Dialogue.mod:1
msgid "Keep swimming in this and I'll have no skin left!"
msgstr "Jeszcze więcej pływania i nie będę mieć skóry!"

